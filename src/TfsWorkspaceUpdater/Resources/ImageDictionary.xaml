<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style TargetType="Image" x:Key="HeaderImageStyle">
        <Setter Property="Height" Value="16"/>
        <Setter Property="Width" Value="16"/>
    </Style>
    
    <Style TargetType="Image" x:Key="CloseImageStyle" BasedOn="{StaticResource HeaderImageStyle}">
        <Setter Property="Source" Value="Images/Close.png"/>
    </Style>

    <Style TargetType="Image" x:Key="TfsServerImageStyle" BasedOn="{StaticResource HeaderImageStyle}">
        <Setter Property="Source" Value="Images/TFSServer.png"/>
    </Style>

    <Style TargetType="Image" x:Key="StartImageStyle">
        <Setter Property="Source" Value="Images/Run.png"/>
    </Style>

</ResourceDictionary>